<keymap version="1" name="bobko-keymap" disable-mnemonics="false" parent="$default">
  <!-- meta is command on macOS -->

  <action id="ActivateUnitTestsToolWindow"/>
  <action id="ActivateNuGetToolWindow"/>
  <action id="ActivateServicesToolWindow"/>
  <action id="ActivateHierarchyToolWindow"/>
  <action id="ActivateStructureToolWindow"/>
  <action id="ActivateBookmarksToolWindow"/>
  <action id="ActivateProblemsViewToolWindow"/>
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="meta 0"/>
    <keyboard-shortcut first-keystroke="alt 0"/>
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="meta 1"/>
    <keyboard-shortcut first-keystroke="alt 1"/>
  </action>
  <action id="ActivateVersionControlToolWindow">
    <keyboard-shortcut first-keystroke="alt 2"/>
    <keyboard-shortcut first-keystroke="meta 2"/>
  </action>
  <action id="ActivateFindToolWindow">
    <keyboard-shortcut first-keystroke="meta 3"/>
    <keyboard-shortcut first-keystroke="alt 3"/>
  </action>
  <action id="ActivateRunToolWindow">
    <keyboard-shortcut first-keystroke="meta 4"/>
    <keyboard-shortcut first-keystroke="alt 4"/>
  </action>
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="meta 5"/>
    <keyboard-shortcut first-keystroke="alt 5"/>
  </action>
  <action id="ActivateBuildToolWindow">
    <keyboard-shortcut first-keystroke="meta 6"/>
    <keyboard-shortcut first-keystroke="alt 6"/>
  </action>

  <action id="ShowNavBar">
    <keyboard-shortcut first-keystroke="alt shift 1"/>
    <keyboard-shortcut first-keystroke="meta shift 1"/>
  </action>

  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="control shift A"/>
    <keyboard-shortcut first-keystroke="alt shift A"/>
    <keyboard-shortcut first-keystroke="meta shift A"/>
  </action>

  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="alt shift F"/>
    <keyboard-shortcut first-keystroke="meta shift F"/>
  </action>

  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="meta ENTER"/>
    <keyboard-shortcut first-keystroke="alt ENTER"/>
  </action>

  <action id="NextOccurence">
    <keyboard-shortcut first-keystroke="alt shift J"/>
  </action>
  <action id="PreviousOccurence">
    <keyboard-shortcut first-keystroke="alt shift K"/>
  </action>

  <action id="Vcs.RollbackChangedLines">
    <keyboard-shortcut first-keystroke="control alt Z"/>
    <keyboard-shortcut first-keystroke="control meta Z"/>
  </action>
  <action id="ChangesView.Revert">
    <keyboard-shortcut first-keystroke="control alt Z"/>
    <keyboard-shortcut first-keystroke="control meta Z"/>
  </action>

  <!-- Unassign control alt I -->
  <action id="AutoIndentLines"/>
  <action id="Inline">
    <keyboard-shortcut first-keystroke="control alt I"/>
    <keyboard-shortcut first-keystroke="control meta I"/>
  </action>
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="control alt V"/>
    <keyboard-shortcut first-keystroke="control meta V"/>
  </action>
  <action id="ExtractMethod">
    <keyboard-shortcut first-keystroke="control alt M"/>
    <keyboard-shortcut first-keystroke="control meta M"/>
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="control alt B"/>
    <keyboard-shortcut first-keystroke="control meta B"/>
  </action>

  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="DOWN"/>
    <keyboard-shortcut first-keystroke="control N"/>
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="UP"/>
    <keyboard-shortcut first-keystroke="control P"/>
  </action>

  <!-- alt shift r is assigned to RerunTests by default. Drop the shortcut -->
  <action id="RerunTests"/>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="meta shift R"/>
    <keyboard-shortcut first-keystroke="alt shift R"/>
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="meta R"/>
    <keyboard-shortcut first-keystroke="alt R"/>
  </action>

  <action id="ChooseDebugConfiguration">
    <keyboard-shortcut first-keystroke="meta shift D"/>
    <keyboard-shortcut first-keystroke="alt shift D"/>
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="meta D"/>
    <keyboard-shortcut first-keystroke="alt D"/>
  </action>

  <action id="TypeHierarchy">
    <keyboard-shortcut first-keystroke="control alt H"/>
    <keyboard-shortcut first-keystroke="control meta H"/>
  </action>
  <action id="GotoSuperMethod">
    <keyboard-shortcut first-keystroke="alt U"/>
    <keyboard-shortcut first-keystroke="meta U"/>
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="alt shift O"/>
    <keyboard-shortcut first-keystroke="meta shift O"/>
  </action>
  <action id="ManageRecentProjects">
    <keyboard-shortcut first-keystroke="alt O"/>
    <keyboard-shortcut first-keystroke="meta O"/>
  </action>
  <!-- I need to break my ctrl+f1 muscle memory -->
  <action id="SelectIn"/>
  <!-- Unassign default control n -->
  <action id="GotoClass"/>
  <action id="OptimizeImports">
    <keyboard-shortcut first-keystroke="control alt O"/>
    <keyboard-shortcut first-keystroke="control meta O"/>
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="alt L"/>
    <keyboard-shortcut first-keystroke="meta L"/>
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="alt H"/>
    <keyboard-shortcut first-keystroke="meta H"/>
  </action>

  <!-- mnemonics: git LLLog -->
  <action id="Vcs.ShowTabbedFileHistory">
    <keyboard-shortcut first-keystroke="alt shift L"/>
    <keyboard-shortcut first-keystroke="meta shift L"/>
  </action>

  <!-- Unassign alt shift c -->
  <action id="RecentChanges" />
  <!-- copy path from repository root -->
  <action id="CopyPathFromRepositoryRootProvider">
    <keyboard-shortcut first-keystroke="alt shift C"/>
    <keyboard-shortcut first-keystroke="meta shift C"/>
  </action>

  <action id="$Copy">
    <keyboard-shortcut first-keystroke="control C"/>
    <keyboard-shortcut first-keystroke="meta C"/>
    <keyboard-shortcut first-keystroke="alt C"/>
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="control V"/>
    <keyboard-shortcut first-keystroke="meta V"/>
    <keyboard-shortcut first-keystroke="alt V"/>
  </action>

  <!-- Unfortunatelly, F keys are more convenient for this :( -->
  <action id="ToggleLineBreakpoint">
    <keyboard-shortcut first-keystroke="alt B"/>
    <keyboard-shortcut first-keystroke="meta B"/>
  </action>
  <action id="ViewBreakpoints">
    <keyboard-shortcut first-keystroke="alt shift B"/>
    <keyboard-shortcut first-keystroke="meta shift B"/>
  </action>
  <action id="EditBreakpoint">
    <keyboard-shortcut first-keystroke="alt shift B"/>
    <keyboard-shortcut first-keystroke="meta shift B"/>
  </action>
  <!-- <action id="SmartStepInto"> -->
  <!--   <keyboard-shortcut first-keystroke="meta shift 7"/> -->
  <!--   <keyboard-shortcut first-keystroke="alt shift 7"/> -->
  <!-- </action> -->
  <!-- <action id="StepInto"> -->
  <!--   <keyboard-shortcut first-keystroke="meta 7"/> -->
  <!--   <keyboard-shortcut first-keystroke="alt 7"/> -->
  <!-- </action> -->
  <!-- <action id="Resume"> -->
  <!--   <keyboard-shortcut first-keystroke="meta 9"/> -->
  <!--   <keyboard-shortcut first-keystroke="alt 9"/> -->
  <!-- </action> -->
  <!-- <action id="StepOver"> -->
  <!--   <keyboard-shortcut first-keystroke="meta 8"/> -->
  <!--   <keyboard-shortcut first-keystroke="alt 8"/> -->
  <!-- </action> -->

  <action id="QuickEvaluateExpression">
    <mouse-shortcut keystroke="alt shift button1"/>
    <mouse-shortcut keystroke="meta shift button1"/>
  </action>
  <!-- kill -->
  <action id="Stop">
    <keyboard-shortcut first-keystroke="meta K"/>
    <keyboard-shortcut first-keystroke="alt K"/>
  </action>
  <action id="StopBackgroundProcesses">
    <keyboard-shortcut first-keystroke="meta shift K"/>
    <keyboard-shortcut first-keystroke="alt shift K"/>
  </action>

  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="alt COMMA"/>
    <keyboard-shortcut first-keystroke="meta COMMA"/>
  </action>
  <action id="ShowProjectStructureSettings">
    <keyboard-shortcut first-keystroke="meta SEMICOLON"/>
    <keyboard-shortcut first-keystroke="alt SEMICOLON"/>
  </action>

  <action id="Diff.NextChange">
    <keyboard-shortcut first-keystroke="alt shift L"/>
    <keyboard-shortcut first-keystroke="meta shift L"/>
  </action>
  <action id="Diff.PrevChange">
    <keyboard-shortcut first-keystroke="alt shift H"/>
    <keyboard-shortcut first-keystroke="meta shift H"/>
  </action>
  <action id="PreviousDiff">
    <keyboard-shortcut first-keystroke="alt shift K"/>
    <keyboard-shortcut first-keystroke="meta shift K"/>
  </action>
  <action id="NextDiff">
    <keyboard-shortcut first-keystroke="alt shift J"/>
    <keyboard-shortcut first-keystroke="meta shift J"/>
  </action>

  <action id="EditorIncreaseFontSizeGlobal">
    <keyboard-shortcut first-keystroke="meta ADD" />
    <keyboard-shortcut first-keystroke="alt ADD" />
    <keyboard-shortcut first-keystroke="meta EQUALS" />
    <keyboard-shortcut first-keystroke="alt EQUALS" />
  </action>
  <action id="EditorDecreaseFontSizeGlobal">
    <keyboard-shortcut first-keystroke="meta SUBTRACT" />
    <keyboard-shortcut first-keystroke="alt SUBTRACT" />
    <keyboard-shortcut first-keystroke="meta MINUS" />
    <keyboard-shortcut first-keystroke="alt MINUS" />
  </action>
  <action id="EditorResetFontSize">
    <keyboard-shortcut first-keystroke="alt 0" />
    <keyboard-shortcut first-keystroke="meta 0" />
  </action>

  <!-- Unassign alt backspace -->
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="meta Z"/>
    <keyboard-shortcut first-keystroke="control Z"/>
  </action>
  <!-- Delete file -->
  <action id="Editor Delete">
    <keyboard-shortcut first-keystroke="DELETE" />
    <keyboard-shortcut first-keystroke="alt back_space" />
    <keyboard-shortcut first-keystroke="meta back_space" />
  </action>
  <action id="$Delete">
    <keyboard-shortcut first-keystroke="DELETE" />
    <keyboard-shortcut first-keystroke="alt back_space" />
    <keyboard-shortcut first-keystroke="meta back_space" />
  </action>

  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="meta W"/>
    <keyboard-shortcut first-keystroke="alt W"/>
  </action>
  <action id="PinActiveTab">
    <keyboard-shortcut first-keystroke="alt P" />
    <keyboard-shortcut first-keystroke="meta P" />
  </action>
  <action id="PinToolwindowTab">
    <keyboard-shortcut first-keystroke="alt P" />
    <keyboard-shortcut first-keystroke="meta P" />
  </action>

  <!-- Vim flavored shortcut -->
  <!-- Related: https://youtrack.jetbrains.com/issue/VIM-44 -->
  <action id="Back">
    <keyboard-shortcut first-keystroke="control O"/>
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="control I"/>
  </action>

  <!-- Unassign control tab -->
  <action id="Switcher" />
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="control tab" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="control shift tab" />
  </action>

  <!-- module settings F4 -->
  <action id="EditSource">
    <keyboard-shortcut first-keystroke="alt E"/>
    <keyboard-shortcut first-keystroke="meta E"/>
  </action>
  <action id="XDebugger.JumpToTypeSource">
    <keyboard-shortcut first-keystroke="alt shift E"/>
    <keyboard-shortcut first-keystroke="meta shift E"/>
  </action>

  <action id="XDebugger.MuteBreakpoints">
    <keyboard-shortcut first-keystroke="alt M"/>
    <keyboard-shortcut first-keystroke="meta M"/>
  </action>

  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt N" />
    <keyboard-shortcut first-keystroke="meta N" />
  </action>
  <!-- Unassign F1 (aka accidential F1 presses that open browser) -->
  <action id="ContextHelp" />
  <!-- reload all external projects -->
  <action id="ExternalSystem.RefreshAllProjects">
    <keyboard-shortcut first-keystroke="control shift alt R" />
    <keyboard-shortcut first-keystroke="control shift meta R" />
  </action>
  <!-- Build current module -->
  <action id="MakeModule">
    <keyboard-shortcut first-keystroke="alt b" />
    <keyboard-shortcut first-keystroke="meta b" />
  </action>
  <!-- Unassign "build project" ctrl F9 -->
  <action id="CompileDirty"/>

  <!-- View Breakpoints... control shift F8 -->

  <!-- Unassign control shift f12 to break muscle memory -->
  <action id="HideAllWindows"/>
  <!-- Unassign control F1 to break muscle memory (I use <space>d in ideavim instead) -->
  <action id="ShowErrorDescription"/>
  <!-- <action id="HideAllWindows"> -->
  <!--   <keyboard-shortcut first-keystroke="control alt V"/> -->
  <!-- </action> -->

  <!-- double alt -->
  <!-- show history of current file -->

  <action id="EditorDeleteToWordStart">
    <keyboard-shortcut first-keystroke="control BACK_SPACE"/>
    <keyboard-shortcut first-keystroke="alt BACK_SPACE"/>
    <keyboard-shortcut first-keystroke="meta BACK_SPACE"/>
  </action>
</keymap>
