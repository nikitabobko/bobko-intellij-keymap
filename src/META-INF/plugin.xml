<idea-plugin>
  <name>Bobko Keymap</name>
  <id>bobko.keymap</id>
  <version>VERSION_PLACEHOLDER</version>
  <vendor>Bobko</vendor>
  <category>Keymap</category>
  <description>Bobko keymap</description>
  <!-- This lines makes plugin compatible with different JB IDEs (not only IJ) -->
  <depends>com.intellij.modules.lang</depends>
  <!-- Install IdeaVim automatically -->
  <depends>IdeaVIM</depends>
  <extensions defaultExtensionNs="com.intellij">
    <bundledKeymap file="bobko-keymap.xml"/>
  </extensions>
</idea-plugin>